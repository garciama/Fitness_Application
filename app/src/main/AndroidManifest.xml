<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.logindemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".Nutrition" />
        <activity android:name=".Workout" />
        <activity android:name=".Notes" />
        <activity android:name=".Timer" />
        <activity
            android:name=".NoteSelect"
            android:label="@string/title_activity_note_select"
            android:theme="@style/AppTheme" />
        <activity android:name=".TestTimer" />
        <activity android:name=".Stopwatch" />
    </application>

</manifest>